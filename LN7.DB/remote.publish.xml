<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>lucky7db</TargetDatabaseName>
    <DeployScriptFileName>LN7.DB.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=lucky7db.database.windows.net;Persist Security Info=True;User ID=luckydb;Pooling=False;Multiple Active Result Sets=False;Connect Timeout=60;Encrypt=True;Trust Server Certificate=False;Command Timeout=0</TargetConnectionString>
    <BlockOnPossibleDataLoss>True</BlockOnPossibleDataLoss>
    <DatabaseEdition>Basic</DatabaseEdition>
    <DatabaseServiceObjective>Basic</DatabaseServiceObjective>
    <DatabaseMaximumSize>2</DatabaseMaximumSize>
    <ProfileVersionNumber>1</ProfileVersionNumber>
  </PropertyGroup>
</Project>